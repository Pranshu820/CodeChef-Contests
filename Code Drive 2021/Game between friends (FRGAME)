#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
    int a, b, c, d;
    cin >> a >> b >> c >> d;
    
    if(b > a) a += c;
    else b += c;

    if(b > a) a += d;
    else b += d;

    if(b > a) cout << "S\n";
    else cout << "N\n";
}
    return 0;
}
