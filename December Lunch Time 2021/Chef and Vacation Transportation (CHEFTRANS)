#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
    int x, y, z;
    cin >> x >> y >> z;
    if(x + y > z) cout << "TRAIN\n";
    else if(x + y < z) cout << "PLANEBUS\n";
    else cout << "EQUAL\n";
}
    return 0;
}
