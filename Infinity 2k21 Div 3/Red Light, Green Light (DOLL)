#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
    int n, k;
    cin >> n >> k;
    int arr[n];
    for(auto &i : arr) cin >> i;
    int c = 0;
    for(int i = 0; i < n; i++){
        if(arr[i] > k) c++;
    }
    cout << c << "\n";
}
    return 0;
}
