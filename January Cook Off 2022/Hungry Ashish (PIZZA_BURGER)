#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
    int x, y, z;
    cin >> x >> y >> z;

    if(x < y && x < z) cout << "NOTHING\n";
    else if(x >= y && x >= z) cout << "PIZZA\n";
    else if(y > x && x >= z) cout << "BURGER\n";
    else if(x >= y && x < z) cout << "PIZZA\n";
}
    return 0;
}
