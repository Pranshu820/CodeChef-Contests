#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
    double a, b, c, d;
    cin >> a >> b >> c >> d;
    if((a * a) / (c * c * c) == (b * b) / (d * d * d)) cout << "YES\n";
    else cout << "NO\n";    
}
    return 0;
}
