#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
    int n;
    cin >> n;
    if(n & 1) cout << "-1\n";
    else{
        for(int i = 1; i <= n/2; i++) cout << '1';
        for(int i = 1; i <= n/2; i++) cout << '0';
        cout << "\n";
    }
}
    return 0;
}
