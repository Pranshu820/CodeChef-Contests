#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
    int z, y, a, b, c;
    cin >> z >> y >> a >> b >> c;
    if(a + b + c <= z - y) cout << "YES\n";
    else cout << "NO\n";
}
    return 0;
}
