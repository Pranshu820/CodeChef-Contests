#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
    int n, m;
    cin >> n >> m;
    
    int x = 0, y = 0;

    if(n & 1) x = n/2 + 1;
    else x = n/2;

    if(m & 1) y = m/2 + 1;
    else y = m/2;

    cout << x * y << "\n";
}
    return 0;
}
