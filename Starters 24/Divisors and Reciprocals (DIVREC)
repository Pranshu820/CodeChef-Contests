#include <bits/stdc++.h>
#define ll long long
#define mod 1000000007
using namespace std;



int main(){
ios_base::sync_with_stdio(false); cin.tie(0);
int t; cin >> t; while(t--){
	ll x, a, b;
	cin >> x >> a >> b;

	if(x == 1 && a == 1 && b == 1) cout << "1\n";
    else if(a <= b) cout << "-1\n";
	else if(x % a) cout << "-1\n";
	else cout << b * (x/a) << "\n";
}
	return 0;
}
